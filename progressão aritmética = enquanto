programa
//Autor Vitor Manoel TI- manhã
{
	
	funcao inicio()
	{
	 
	inteiro numero,resultado,contador
	
	 escreva ("Ente com numero para tabuada :")
	 
	 leia (numero)
	 contador=1
	 enquanto (contador <10)
	 {
	 	resultado =numero * contador
	 	escreva (numero,"X",contador,"=",resultado,"\n")
	 	contador=contador ++
	 	
	
	    }
     }
}
